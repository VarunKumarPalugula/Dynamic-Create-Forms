{
  "name": "lawfront",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "postinstall": "npm run prettier -s",
    "precommit": "pretty-quick --staged",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/http": "^7.2.16",
    "@angular/localize": "^9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@angular/service-worker": "^9.1.9",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@kolkov/angular-editor": "^1.1.2",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "@ng-select/ng-select": "^4.0.0",
    "@ngx-translate/core": "^12.1.2",
    "@types/file-saver": "^2.0.1",
    "angular-editor": "0.0.3",
    "angular-randomcolor": "^1.0.3",
    "angular-user-idle": "^2.2.2",
    "bootstrap": "^4.5.0",
    "core-js": "^2.5.7",
    "file-saver": "^2.0.2",
    "jquery": "^3.5.1",
    "ng-circle-progress": "^1.5.1",
    "ng-multiselect-dropdown": "^0.2.10",
    "ng2-file-upload": "^1.4.0",
    "ng2-pdf-viewer": "^6.3.0",
    "ng2-search-filter": "^0.5.1",
    "ng2-signalr": "^8.0.2",
    "ngx-bootstrap": "^5.6.1",
    "ngx-file-drop": "^9.0.1",
    "ngx-filter-pipe": "^2.1.2",
    "ngx-order-pipe": "^2.0.4",
    "ngx-pagination": "^5.0.0",
    "ngx-popper": "^7.0.0",
    "ngx-signalr-hubservice": "^9.0.1",
    "ngx-spinner": "^9.0.2",
    "ngx-toastr": "^12.1.0",
    "ngx-tour-core": "^4.1.1",
    "ngx-tour-ngx-popper": "^4.1.1",
    "uppy": "1.24.0",
    "uppy-angular": "0.1.4",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.5",
    "signalr": "^2.4.1",
    "xlsx": "^0.16.2",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular/cli": "^9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@biesbjerg/ngx-translate-extract": "^7.0.0",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.11",
    "codelyzer": "^5.2.2",
    "hads": "^2.1.0",
    "htmlhint": "^0.14.0",
    "https-proxy-agent": "^5.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "~5.0.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^3.3.1",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "~7.0.0",
    "stylelint": "~13.6.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.17.2",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
